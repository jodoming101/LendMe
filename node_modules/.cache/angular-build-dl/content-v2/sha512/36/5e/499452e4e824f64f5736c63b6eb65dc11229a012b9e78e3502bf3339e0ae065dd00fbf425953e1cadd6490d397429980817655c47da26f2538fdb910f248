class MyProjectShowCompanyOwnerComponent {
    constructor(router, route, cookie, apiService, ngxService, datePipe, sanitizer) {
        this.router = router;
        this.route = route;
        this.cookie = cookie;
        this.apiService = apiService;
        this.ngxService = ngxService;
