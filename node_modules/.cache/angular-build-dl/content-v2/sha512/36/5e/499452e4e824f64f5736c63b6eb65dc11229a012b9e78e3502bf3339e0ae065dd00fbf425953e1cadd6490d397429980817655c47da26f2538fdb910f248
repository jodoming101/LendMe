class ProjectEditCompanyOwnerComponent {
    constructor(formBuilder, router, route, cookie, apiService, ngxService, datePipe, sanitizer) {
        this.formBuilder = formBuilder;
        this.router = router;
        this.route = route;
        this.cookie = cookie;
        this.apiService = apiService;
        this.ngxService = ngxService;
        this.datePipe = datePipe;
        this.sanitizer = sanitizer;
        this.infosUser = new _interfaces_models__WEBPACK_IMPORTED_MODULE_1__["UserModel"]();
        this.ObjetProject = new _interfaces_models__WEBPACK_IMPORTED_MODULE_1__["ProjectModel"]();
        this.ObjetProjectTemplate = new _interfaces_models__WEBPACK_IMPORTED_MODULE_1__["templteProjectModelBis"]();
        this.imagesProjects = [];
        this.arrayAdressReseauxSociauxProject = [];
        this.listCategorieProject = [];
        this.listPorteProject = [];
        this.listeStatusProject = [
            { key: 1, value: 'Valider le porjet' },
            { key: 2, value: 'Terminer le porjet' },
            { key: 3, value: 'Annuler le porjet' }
        ];
        this.photoUserAdmin = './assets/img/users/user_f.png';
        this.submitted = false;
        this.checkContrePartie = false;
        this.isvalidCaptcha = true; // test
        this.isErreurCaptcha = false;
        this.isErreurValidProject = false;
        this.datePickerConfig = {
            drops: 'up',
            format: 'YYYY-MM-DD',
            locale: 'fr',
            addClass: 'form-control',
        };
        if (this.cookie.get('infosUser')) {
            this.infosUser = JSON.parse(this.cookie.get('infosUser'));
            if (this.infosUser.photoUser === '' || !this.infosUser.photoUser) {
                if (this.infosUser.sex === 'F') {
                    this.infosUser.photoUser = './assets/img/users/user_f.png';
                }
                if (this.infosUser.sex === 'H') {
                    this.infosUser.photoUser = './assets/img/users/user_m.png';
                }
            }
            this.route.params.subscribe(params => {
                this.getinfosProject(params.token);
            });
            console.log('ProfilUserComponent', this.infosUser);
        }
        else {
            this.router.navigate(['/Identification']);
        }
    }
    ngOnInit() {
        this.updateProjectForm = this.formBuilder.group({
            nomProject: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            descriptionProject: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            porteProject: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            categorieProject: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            // tslint:disable-next-line:max-line-length
            montantMinimunProject: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required]],
            dateLimitCollectProject: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].pattern('[0-9]{4}-[0-9]{2}-[0-9]{2}')]],
        });
        //  this.addRecaptchaScript();
        this.getListCategorieProject();
        this.getListPorteProject();
    }
    addEventDateLimiteCollecte(event) {
        const date = new Date();
        if (event < date) {
            this.isErreurValidProject = true;
        }
        else {
            this.isErreurValidProject = false;
            this.ObjetProject.date_limite_collecte = this.datePipe.transform(event.value, 'yyyy-MM-dd');
            console.log(this.ObjetProject.date_limite_collecte);
        }
    }
    handleChange(value) {
        this.ObjetProject.contrePartieProject = value;
        this.checkContrePartie = false;
    }
    onChangeCategorieProject(index) {
        /* console.log('objectCategorie', this.listCategorieProject[index]);
     
         this.ObjetProject.categoryProject = this.listCategorieProject[index]; */
    }
    onChangePorteProject(index) {
        /*  console.log('objectPorte', this.listPorteProject[index]);
      
          this.ObjetProject._porte_project = this.listPorteProject[index];*/
    }
    get f() { return this.updateProjectForm.controls; }
    onFormSubmitUpdateProject() {
        this.submitted = true;
        if (!this.isvalidCaptcha) {
            this.isErreurCaptcha = true;
        }
        if (this.ObjetProject.contrePartieProject === undefined) {
            this.checkContrePartie = true;
        }
        if (this.updateProjectForm.invalid) {
            return;
        }
        if (this.ObjetProject.contrePartieProject && this.isvalidCaptcha && !this.checkContrePartie) {
            this.apiService.updateDataProjectByUser(this.ObjetProject).subscribe((data) => {
            }, (error) => {
            });
        }
    }
    getListCategorieProject() {
        this.apiService.getListCategorieProject().subscribe((data) => {
            console.log(data);
            this.listCategorieProject = data;
        }, (error) => {
        });
    }
    getListPorteProject() {
        this.apiService.getListPorteProject().subscribe((data) => {
            console.log(data);
            this.listPorteProject = data;
        }, (error) => {
        });
    }
    getinfosProject(tokenProject) {
        this.ngxService.start();
        this.apiService.getDataProject(tokenProject).subscribe((dataPorject) => {
            console.log('dataPorject = ', dataPorject);
            //  this.ObjetProjectTemplate.project = dataPorject;
            this.ObjetProject = dataPorject;
            this.getListArrayAdressReseauxSociauxProject();
            this.getAllImageProject();
            this.ngxService.stop();
        }, (error) => {
            this.ngxService.stop();
        });
    }
    addRecaptchaScript() {
        window.grecaptchaCallback = () => {
            this.renderReCaptcha();
        };
        (function (d, s, id, obj) {
            let js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {
                obj.renderReCaptcha();
                return;
            }
            js = d.createElement(s);
            js.id = id;
            js.src = 'https://www.google.com/recaptcha/api.js?onload=grecaptchaCallback&amp;render=explicit';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'recaptcha-jssdk', this));
    }
    renderReCaptcha() {
        window.grecaptcha.render(this.recaptchaElement.nativeElement, {
            sitekey: '6Lf4I6gZAAAAAMp1E9YI1FJghdQ20CNRtAV9d55y',
            callback: (response) => {
                console.log('response', response);
                this.isvalidCaptcha = true;
                this.isErreurCaptcha = false;
            }
        });
    }
    getListArrayAdressReseauxSociauxProject() {
        /* this.apiService.getListArrayAdressReseauxSociauxProject(this.ObjetProjectTemplate.project).subscribe((data: any) => {
    
    
